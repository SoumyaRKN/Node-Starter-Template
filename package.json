{
	"type": "module",
	"license": "MIT",
	"version": "1.0.0",
	"main": "src/main.js",
	"name": "node-starter-template",
	"author": "Soumya Prakash Nayak",
	"description": "This is Node Starter Template.",
	"directories": {
		"src": "src",
		"doc": "docs",
		"log": "logs",
		"test": "tests",
		"infra": "infra",
		"script": "scripts",
		"seeder": "seeders",
		"migration": "migrations"
	},
	"scripts": {
		"test": "jest --runInBand",
		"lint": "eslint . --ext .js",
		"format": "prettier --write .",
		"ci": "npm run lint && npm test",
		"seed": "node --env-file=.env scripts/seed.js",
		"migrate:seed": "npm run migrate && npm run seed",
		"dev": "node --env-file=.env scripts/init-dev.js",
		"migrate": "node --env-file=.env scripts/migrate.js",
		"vault:init": "node --env-file=.env scripts/vault/init.js",
		"make:seeder": "node --env-file=.env scripts/make-seeder.js",
		"seed:rollback": "node --env-file=.env scripts/seed-rollback.js",
		"start": "node --env-file=.env --enable-source-maps src/main.js",
		"make:migration": "node --env-file=.env scripts/make-migration.js",
		"migrate:rollback": "node --env-file=.env scripts/migrate-rollback.js",
		"migrate:seed:rollback": "npm run seed:rollback && npm run migrate:rollback"
	},
	"dependencies": {
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"bcrypt": "^6.0.0",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"joi": "^18.0.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.17.1",
		"morgan": "^1.10.1",
		"mysql2": "^3.14.3",
		"node-cron": "^4.2.1",
		"node-vault": "^0.10.5",
		"pg": "^8.16.3",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.37.7",
		"swagger-ui-express": "^5.0.1",
		"umzug": "^3.8.2",
		"uuid": "^11.1.0",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"eslint": "^9.33.0",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"jest": "^30.0.5",
		"lint-staged": "^16.1.5",
		"nodemon": "^3.1.10",
		"prettier": "^3.6.2",
		"sequelize-cli": "^6.6.3",
		"supertest": "^7.1.4"
	}
}
